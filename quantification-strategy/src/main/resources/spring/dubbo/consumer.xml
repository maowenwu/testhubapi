<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dubbo="http://dubbo.apache.org/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd http://dubbo.apache.org/schema/dubbo http://dubbo.apache.org/schema/dubbo/dubbo.xsd">

    <dubbo:application name="consumer-of-helloworld-app"/>
    <dubbo:registry address="zookeeper://127.0.0.1:2181"/>

    <dubbo:reference id="accountService" interface="com.huobi.quantification.api.AccountService"/>
    <dubbo:reference id="marketService" interface="com.huobi.quantification.api.MarketService"/>
    <dubbo:reference id="orderService" interface="com.huobi.quantification.api.OrderService"/>
    <dubbo:reference id="jobManageService" interface="com.huobi.quantification.api.JobManageService"/>
</beans>