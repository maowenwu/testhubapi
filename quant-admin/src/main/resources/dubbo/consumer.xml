<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dubbo="http://dubbo.apache.org/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd http://dubbo.apache.org/schema/dubbo http://dubbo.apache.org/schema/dubbo/dubbo.xsd">

    <dubbo:application name="admin-consumer"/>

    <dubbo:registry protocol="zookeeper" address="${zookeeper.address}"/>

    <dubbo:reference id="contractService" interface="com.huobi.quantification.api.future.FutureContractService"/>
    <dubbo:reference id="accountService" interface="com.huobi.quantification.api.future.FutureAccountService"/>
    <dubbo:reference id="marketService" interface="com.huobi.quantification.api.future.FutureMarketService"/>
    <dubbo:reference id="orderService" interface="com.huobi.quantification.api.future.FutureOrderService"/>
    <dubbo:reference id="jobManageService" interface="com.huobi.quantification.api.future.JobManageService"/>


    <dubbo:reference id="spotAccountService" interface="com.huobi.quantification.api.spot.SpotAccountService"/>
    <dubbo:reference id="spotMarketService" interface="com.huobi.quantification.api.spot.SpotMarketService"/>
    <dubbo:reference id="spotOrderService" interface="com.huobi.quantification.api.spot.SpotOrderService"/>

    <dubbo:reference id="emailService" interface="com.huobi.quantification.api.email.EmailService"/>

</beans>