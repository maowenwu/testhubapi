<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.huobi.quantification.dao.QuanDepthMapper">
  <resultMap id="BaseResultMap" type="com.huobi.quantification.entity.QuanDepth">
    <result column="id" jdbcType="BIGINT" property="id" />
    <result column="depth_status" jdbcType="VARCHAR" property="depthStatus" />
    <result column="depth_exchange_id" jdbcType="BIGINT" property="depthExchangeId" />
    <result column="depth_ts" jdbcType="BIGINT" property="depthTs" />
    <result column="depth_ch" jdbcType="VARCHAR" property="depthCh" />
  </resultMap>
  <insert id="insert" parameterType="com.huobi.quantification.entity.QuanDepth">
    insert into quan_depth (id, depth_status, depth_exchange_id, 
      depth_ts, depth_ch)
    values (#{id,jdbcType=BIGINT}, #{depthStatus,jdbcType=VARCHAR}, #{depthExchangeId,jdbcType=BIGINT}, 
      #{depthTs,jdbcType=BIGINT}, #{depthCh,jdbcType=VARCHAR})
  </insert>
  <select id="selectAll" resultMap="BaseResultMap">
    select id, depth_status, depth_exchange_id, depth_ts, depth_ch
    from quan_depth
  </select>
</mapper>